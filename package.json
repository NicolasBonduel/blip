{
  "name": "@lime.it/blip",
  "description": "Project facilitator for people with little time and little desire to work",
  "version": "0.1.2",
  "author": "gcanossa @gcanossa",
  "bin": {
    "blip": "./bin/run"
  },
  "bugs": "https://github.com/lime-it/blip/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "execa": "^4.0.0",
    "inquirer": "^7.0.4",
    "listr": "^0.14.3",
    "selfsigned": "^1.10.7",
    "tslib": "^1.10.0",
    "uninstall": "0.0.0",
    "uuid": "^3.4.0",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.8",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.14",
    "@types/uuid": "^3.4.7",
    "@types/yaml": "^1.2.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "install": "^0.13.0",
    "mocha": "^5.2.0",
    "npm": "^6.13.7",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/assets",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/lime-it/blip",
  "keywords": [
    "oclif"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "blip",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "lime-it/blip",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json && rimraf assets",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme && cp -r src/assets assets",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
